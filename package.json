{
  "name": "@richard-stovall/inat-typescript-client",
  "version": "0.1.0",
  "description": "TypeScript client for iNaturalist API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "clean": "rm -rf dist",
    "prebuild": "bun run clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "echo 'No tests specified yet' && exit 0",
    "generate:modules": "bun generators/generate-modules.js",
    "generate:docs": "bun generators/generate-docs.js",
    "generate:postman": "bun generators/generate-inat-postman-collection.js",
    "generate:sdk": "bun generators/generate-sdk.js",
    "parse:swagger": "bun generators/parse-swagger-specs.js"
  },
  "dependencies": {
    "axios": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/bun": "^1.2.18",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "rollup": "^4.44.2",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.2.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "@apidevtools/swagger-parser": "^12.0.0",
    "cmd-ts": "^0.13.0",
    "ejs": "^3.1.10",
    "js-yaml": "^4.1.0"
  }
}
