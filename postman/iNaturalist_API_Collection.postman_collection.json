          },
          "response": []
        },
        {
          "name": "POST token",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "description": "OAuth token exchange endpoint - exchange authorization code for access token",
            "url": "{{inat_base_url}}/oauth/token?client_id=&code=&redirect_uri=&grant_type=&code_verifier=",
            "body": {
              "mode": "raw",
              "raw": "{}",
              "options": {
                "raw": {
                  "language": "json"
