      "name": "Authentication",
      "item": [
        {
          "name": "GET authorize",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}",
                "type": "text"
              }
            ],
            "description": "OAuth authorization endpoint - redirects user to authorize your application",
            "url": "{{inat_base_url}}/oauth/authorize?client_id=&redirect_uri=&response_type=&code_challenge=&code_challenge_method=&scope="
          },
          "response": []
        },
        {
          "name": "GET api_token",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}",
                "type": "text"
              }
